{
  "name": "dummy-api",
  "api_id": "dummy-api-v1",
  "org_id": "default",
  "use_keyless": true,
  "auth": {
    "use_param": false,
    "use_cookie": false,
    "auth_header_name": "Authorization"
  },
  "proxy": {
    "target_url": "http://backend:3001",
    "listen_path": "/dummy-proxy/",
    "strip_listen_path": true
  },
  "version_data": {
    "not_versioned": true,
    "versions": {
      "default": {
        "name": "default",
        "use_extended_paths": true
      }
    }
  },
  "custom_middleware": {
    "pre": [
      {
        "name": "add-channel-header",
        "path": "middleware/add-channel.js"
      }
    ],
    "driver": "javascript"
  },
  "active": true
}