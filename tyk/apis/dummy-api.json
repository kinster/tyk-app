{
  "name": "dummy-api",
  "api_id": "dummy-api-v1",
  "org_id": "evri-org",
  "listen_port": 0,
  "protocol": "http",
  "enable_proxy_protocol": false,
  "use_keyless": false,
  "auth": {
    "auth_header_name": "Authorization"
  },

  "version_data": {
    "not_versioned": true,
    "versions": {
      "default": {
        "name": "default",
        "expires": "",
        "use_extended_paths": true
      }
    }
  },

  "proxy": {
    "target_url": "http://backend.dummy.local:3001",
    "listen_path": "/dummy-proxy/",
    "strip_listen_path": true
  },

  "custom_middleware": {
    "pre": [
      {
        "name": "add-channel-header",
        "path": "middleware/add-channel.js"
      }
    ],
    "driver": "javascript"
  }
}